{
	"Comment": "A simple minimal example of the States language",
	"StartAt": "Hello World",
	"States": {
		"Hello World": {
			"Type": "Task",
			"Resource": "func_nodeinfo",
			"Next": "Goodbye World"
		},
		"Goodbye World": {
			"Type": "Task",
			"Resource": "func_wordcount",
			"Next": "Wait ten seconds"
		},
		"Wait ten seconds": {
			"Type": "Wait",
			"Seconds": 2,
			"Next": "Hash it out"
		},
		"In Parallel": {
			"Type": "Parallel",
			"Branches": [
				{
					"StartAt": "Echo",
					"States": {
						"Echo": {
							"Type": "Task",
							"Resource": "func_echoit",
							"End": true
						}
					}
				},
				{
					"StartAt": "Word Count",
					"States": {
						"Word Count": {
							"Type": "Task",
							"Resource": "func_wordcount",
							"End": true
						}
					}
				}
			],
			"End": true
		},
		"Hash it out": {
			"Type": "Task",
			"Resource": "func_base64",
			"End": true
		}
	}
}
